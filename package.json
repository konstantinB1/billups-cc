{
    "name": "billups",
    "version": "1.0.0",
    "scripts": {
        "client:dev": "npm run dev -w packages/client",
        "api:dev": "npm run dev -w packages/api",
        "migrations:init": "npm run init -w packages/migrations",
        "migrations:create": "npm run create -w packages/migrations",
        "migrations:migrate": "npm run migrate -w packages/migrations",
        "migrations:rollback": "npm run rollback -w packages/migrations",
        "dev": "npm run migrations:migrate && npm run api:dev && npm run client:dev"
    },
    "author": "Konstantin Bulatovic",
    "license": "ISC",
    "workspaces": [
        "packages/*"
    ],
    "devDependencies": {
        "prettier": "^3.3.2",
        "typescript": "^5.5.3"
    }
}
